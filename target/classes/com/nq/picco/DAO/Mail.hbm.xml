<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE hibernate-mapping PUBLIC  
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
<hibernate-mapping package="com.nq.picco.DAO">
  
    <class name="com.nq.picco.model.Mail" table="picco_web_Email">
   		 <id name="id" column="id"><generator class="native" />  </id>
        <property name="name" column="name">  </property>
        
         <property name="subject" column="subject">  </property>  
         
          <property name="email" column="mail">  </property>  
          
           <property name="message" column="message"> </property>  
            
            
      

    </class>  
  
</hibernate-mapping>  